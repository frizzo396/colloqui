<?xml version="1.0" encoding="utf-8" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="liquibase" id="create_table_interviewer_1">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="interviewer" />
        </preConditions>
        <dropTable cascadeConstraints="true" tableName="interviewer" />
    </changeSet>

    <changeSet author="liquibase" id="create_table_interviewer_2">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="interviewer"></tableExists>
            </not>
        </preConditions>
        <createTable tableName="interviewer">
            <column name="ID" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="INTERVIEWER_PK"/>
            </column>
            <column name="ENTERPRISE_ID" type="VARCHAR2(255 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="MAIL" type="VARCHAR2(255 BYTE)">
                <constraints nullable="false"/>
            </column>
             <column name="STATUS" type="NUMBER">
                <constraints nullable="false"/>
            </column>
             <column name="PASSWORD" type="VARCHAR2(255 BYTE)">
                <constraints nullable="false"/>
            </column>
             <column name="TYPE" type="NUMBER">
                <constraints nullable="false"/>
            </column>
            <column name="MANAGEMENT" type="NUMBER">
            </column>
        </createTable>
    </changeSet>


</databaseChangeLog>